    "test_name": "missing_value",
    "description": "Query missing the value after 'is'",
    "input": "prof is",
    "should_succeed": false,
    "expected_error_type": "MissingToken",
    "expected_error_message": "Missing: Expected identifier",
    "expected_problematic_tokens": []
  },
  {
    "test_name": "missing_entity",
    "description": "Query missing the entity before 'is'",
    "input": "is Alan",
    "should_succeed": false,
    "expected_error_type": "InvalidContext",
    "expected_error_message": "'is' is not valid here. Try: 'professor', 'course', 'subject', 'title', 'section', 'number', 'description', 'credit', 'prerequisites', 'corequisites', 'enrollment', 'campus', 'meeting'",
    "expected_problematic_tokens": [
      {
        "lexeme": "is",
        "start": 0,
        "end": 2
      }
    ]
  },
